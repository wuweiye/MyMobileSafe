<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:wuwei="http://schemas.android.com/apk/res/com.wuwei.mymobliesafe"
    android:id="@+id/activity_setting2_action"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.wuwei.mymobliesafe.act.Setting2Activity"

    tools:ignore="ResAuto">

    <TextView
        style="@style/TitleBarTextView"
        android:layout_marginBottom="10dp"
        android:text="设置中心"/>

    <com.wuwei.mymobliesafe.view.SettingItem2View
        android:id="@+id/sit_update"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        wuwei:wbackground="first"
        wuwei:title="自动更新">
    </com.wuwei.mymobliesafe.view.SettingItem2View>

    <com.wuwei.mymobliesafe.view.SettingItem2View
        android:id="@+id/siv_address"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        wuwei:wbackground="middle"
        wuwei:title="电话归属地显示设置">
    </com.wuwei.mymobliesafe.view.SettingItem2View>

    <com.wuwei.mymobliesafe.view.SettingItem2View
        android:id="@+id/siv_blacknumber"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        wuwei:wbackground="middle"
        wuwei:title="黑名单拦截设置">
    </com.wuwei.mymobliesafe.view.SettingItem2View>
    <com.wuwei.mymobliesafe.view.SettingItem2View
        android:id="@+id/siv_app_lock"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        wuwei:wbackground="last"
        wuwei:title="程序锁设置">
    </com.wuwei.mymobliesafe.view.SettingItem2View>

</LinearLayout>
