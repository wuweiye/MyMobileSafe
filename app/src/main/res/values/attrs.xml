<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ShimmerView">
        <attr name="reflectionColor" format="color"/>
    </declare-styleable>
    
    <declare-styleable name="com.wuwei.mymobliesafe.view.SettingItemView">
        <attr name="destitle" format="string"/>
        <attr name="desoff" format="string"/>
        <attr name="deson" format="string"/>
